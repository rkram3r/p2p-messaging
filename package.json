{
  "name": "p2p-messaging",
  "version": "1.0.0",
  "description": "fully-distributed, P2P messaging application.",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "node ./server.js",
    "start:client": "parcel ./src/index.html --open"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.2.1",
    "js-sha256": "^0.9.0",
    "react": "^16.7.0",
    "react-dom": "^16.6.0",
    "react-feather": "^1.1.4",
    "react-router-dom": "^4.3.1",
    "simple-peer": "^9.1.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/simple-peer": "^6.1.5",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.0.1",
    "parcel": "^1.10.3",
    "sass": "^1.16.1",
    "typescript": "^3.3.1"
  }
}
